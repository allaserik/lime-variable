{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=development pnpm nest start",
    "start:dev": "cross-env NODE_ENV=development pnpm start --watch",
    "start:debug": "cross-env NODE_ENV=development pnpm nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production pnpm node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "cd ../ && husky backend/.husky",
    "prisma:dev:push": "dotenv -e .env.development -- prisma db push",
    "prisma:test:push": "dotenv -e .env.test -- prisma db push",
    "prisma:prod:push": "dotenv -e .env.production -- prisma db push",
    "prisma:migrate:dev": "dotenv -e .env.development -- prisma migrate dev ",
    "prisma:migrate:test": "dotenv -e .env.test -- pnpm exec prisma migrate dev --name init",
    "prisma:migrate:prod": "dotenv -e .env.production -- prisma migrate deploy",
    "prisma:generate:dev": "dotenv -e .env.development -- prisma generate",
    "prisma:generate:test": "dotenv -e .env.test -- prisma generate",
    "prisma:generate:prod": "dotenv -e .env.production -- prisma generate",
    "prisma:reset:dev": "dotenv -e .env.development -- prisma migrate reset --force",
    "docker:dev:up": "docker compose -f docker-compose.dev.yml --env-file .env.development -p lime-variable-dev up -d",
    "docker:dev:down": "docker compose -f docker-compose.dev.yml --env-file .env.development -p lime-variable-dev down --volumes ",
    "docker:test:up": "docker compose -f docker-compose.test.yml --env-file .env.test -p lime-variable-test up -d",
    "docker:test:down": "docker compose -f docker-compose.test.yml --env-file .env.test -p lime-variable-test down --volumes ",
    "docker:prod:up": "docker compose -f docker-compose.prod.yml --env-file .env.production -p lime-variable-prod up -d",
    "docker:prod:down": "docker compose -f docker-compose.prod.yml --env-fie .env.production -p lime-variable-prod down --volumes ",
    "docker:start:dev": "pnpm docker:dev:up && pnpm prisma:dev:push && pnpm prisma:generate:dev && pnpm start:dev",
    "docker:start:test": "pnpm docker:test:up && pnpm prisma:test:push && pnpm prisma:generate:test && pnpm test:e2e"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "6.11.1",
    "argon2": "^0.43.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cross-env": "^7.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "pactum": "^3.8.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^6.11.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "lint-staged": {
    "*.ts": [
      "pnpm --dir backend exec prettier --write",
      "pnpm --dir backend exec eslint --config ./eslint.config.mjs --fix"
    ],
    "*.{md,json}": [
      "pnpm --dir backend exec prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
